{
    "extends": ["next/core-web-vitals", "eslint:recommended"],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "quotes": ["error", "single"],
      "jsx-quotes": ["error", "prefer-double"],
      "semi": ["error", "always"],
      "indent": ["error", 2],
      "react/prop-types": "off",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  }